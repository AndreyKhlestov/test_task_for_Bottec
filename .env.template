# Переменные для PostgreSQL
POSTGRES_DB=название БД
POSTGRES_USER=логин пользователя для пдоступа к БД
POSTGRES_PASSWORD=пароль от пользователя

# Переменные для Django-проекта:
SECRET_KEY=секретный ключ Django проекта
DB_HOST=адрес хоста, на котором находится БД (например localhost (если запускаем локально) или название контейнера (если БД запущена в Docker))
DB_PORT=порт, на котором доступна БД (например 5432)

# Переменные для телеграм бота
BOT_TOKEN=токен бота
REDIS_HOST=адрес хоста, на котором находится Redis (например localhost или название контейнера (если БД запущена в Docker))
REDIS_PORT=порт, на котором доступен Redis (например 6379)

# Ссылки
LINK_GROUP = @ссылка на группу
LINK_CHANNEL = @ссылка на канал

# ukassa
PROVIDER_TOKEN=<provider_token> # можно взять у BotFather, bot settings
UKASSA_SECRET_KEY=<secret_key> # в админке Юкассы
SHOP_ID=<shop_id> # в админке Юкассы

